def func():
    N = int(input())
 
    counter = 0
    flag = False
    for i in range(N):
        x, y = map(int, input().split())
        if x == y:
            counter += 1
 
            if counter >= 3:
                flag = True
 
        else:
            counter = 0
 
    if flag:
        print("Yes")
    else:
        print("No")
 
 
# Press the green button in the gutter to run the script.
if __name__ == '__main__':
    func()
