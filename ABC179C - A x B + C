def func():
    N = int(input())
    counter = 0
 
    for a in range(1, N):
        for b in range(a, N):
            if a * b < N:
                if a == b:
                    counter += 1
                else:
                    counter += 2
 
            else:
                break
 
    print(counter)
 
 
# Press the green button in the gutter to run the script.
if __name__ == '__main__':
    func()
