n500 = int(input())
n100 = int(input())
n50 = int(input())
x = int(input())
 
counter = 0
 
for i in range(0,n500 + 1):
    for j in range(0, n100 + 1):
        for k in range(0, n50 + 1):
            #print(i,j,k)
            if (i * 500 + j * 100 + k * 50) == x:
                counter += 1
 
 
print(counter)
